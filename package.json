{
  "name": "dinodns",
  "version": "0.0.4",
  "description": "A pure Typescript, low-configuration full DNS server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jafayer/DinoDNS.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "dns",
    "server",
    "typescript"
  ],
  "author": "Josh Fayer <contact@fayer.me>",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs:build": "typedoc --out docs ./src",
    "docs:clean": "rm -rf docs",
    "docs:serve": "npx http-server docs -o",
    "lint": "eslint 'src/**/*.{js,ts}' --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build:prod",
    "postpublish": "git push --follow-tags",
    "publish": "npm publish"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/dns-packet": "^5.6.5",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "tsx": "^4.10.5",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "dns-packet": "^5.6.1"
  }
}
