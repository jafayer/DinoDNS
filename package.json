{
  "name": "dinodns",
  "version": "0.0.5",
  "description": "A pure Typescript, low-configuration full DNS server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jafayer/DinoDNS.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://www.dinodns.dev",
  "keywords": [
    "dns",
    "server",
    "typescript"
  ],
  "author": "Josh Fayer <contact@fayer.me>",
  "scripts": {
    "build": "tsc -b -v",
    "build:clean": "rm -rf dist && tsc -b -v --incremental false",
    "dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs:build": "typedoc --out docs ./src",
    "docs:clean": "rm -rf docs",
    "docs:serve": "npx http-server docs -o",
    "lint": "eslint",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build:clean",
    "postpublish": "git push --follow-tags",
    "publish": "npm publish"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/dns-packet": "^5.6.5",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tslib": "^2.7.0",
    "tsx": "^4.19.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  },
  "dependencies": {
    "dns-packet": "^5.6.1"
  }
}
